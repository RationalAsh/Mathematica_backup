Cell[BoxData[
 RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"t", ">", "tMax"}], ",", 
      RowBox[{"t", "=", "tMax"}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Quiet", "@", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "model", ",", "gains", ",", "perturbingForce", ",", "controlForce", 
         ",", "sols", ",", "bob", ",", "pivot", ",", "f", ",", "x", ",", 
         "\[Theta]", ",", "\[Theta]0"}], "}"}], ",", 
       RowBox[{
        RowBox[{"\[Theta]0", "=", 
         RowBox[{"\[Theta]s", "+", "\[Theta]e"}]}], ";", 
        RowBox[{
         RowBox[{"model", "[", "t_", "]"}], "=", 
         RowBox[{"StateSpaceModel", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"L", " ", "m", " ", 
               RowBox[{"Sin", "[", 
                RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
               SuperscriptBox[
                RowBox[{
                 SuperscriptBox["\[Theta]", "\[Prime]",
                  MultilineFunction->None], "[", "t", "]"}], "2"]}], 
              "\[Equal]", 
              RowBox[{
               RowBox[{"2", " ", 
                RowBox[{"(", 
                 RowBox[{"c", "+", "m"}], ")"}], " ", 
                RowBox[{
                 SuperscriptBox["x", "\[Prime]\[Prime]",
                  MultilineFunction->None], "[", "t", "]"}]}], "+", 
               RowBox[{"L", " ", "m", " ", 
                RowBox[{"Cos", "[", 
                 RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
                RowBox[{
                 SuperscriptBox["\[Theta]", "\[Prime]\[Prime]",
                  MultilineFunction->None], "[", "t", "]"}]}], "+", 
               RowBox[{"f", "[", "t", "]"}]}]}], ",", 
             RowBox[{
              RowBox[{
               RowBox[{"2", " ", 
                RowBox[{"Sin", "[", 
                 RowBox[{"\[Theta]", "[", "t", "]"}], "]"}]}], "+", 
               RowBox[{"2", " ", 
                RowBox[{"Cos", "[", 
                 RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
                RowBox[{
                 SuperscriptBox["x", "\[Prime]\[Prime]",
                  MultilineFunction->None], "[", "t", "]"}]}], "+", 
               RowBox[{"L", " ", 
                RowBox[{
                 SuperscriptBox["\[Theta]", "\[Prime]\[Prime]",
                  MultilineFunction->None], "[", "t", "]"}]}]}], "\[Equal]", 
              "0"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"x", "[", "t", "]"}], ",", "x0"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                SuperscriptBox["x", "\[Prime]",
                 MultilineFunction->None], "[", "t", "]"}], ",", "x00"}], 
              "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"\[Theta]", "[", "t", "]"}], ",", "\[Theta]0"}], "}"}],
              ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                SuperscriptBox["\[Theta]", "\[Prime]",
                 MultilineFunction->None], "[", "t", "]"}], ",", "0"}], 
              "}"}]}], "}"}], ",", 
           RowBox[{"f", "[", "t", "]"}], ",", 
           RowBox[{"{", "}"}], ",", "t"}], "]"}]}], ";", 
        RowBox[{
         RowBox[{"gains", "[", "t_", "]"}], "=", 
         RowBox[{"-", 
          RowBox[{"First", "[", 
           RowBox[{"LQRegulatorGains", "[", 
            RowBox[{
             RowBox[{"N", "[", 
              RowBox[{"model", "[", "t", "]"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"DiagonalMatrix", "[", 
                RowBox[{"{", 
                 RowBox[{"1", ",", "10", ",", "10", ",", "100"}], "}"}], 
                "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"{", "1", "}"}], "}"}]}], "}"}]}], "]"}], "]"}]}]}], 
        ";", 
        RowBox[{
         RowBox[{"perturbingForce", "[", "t_", "]"}], "=", 
         RowBox[{"Plus", "@@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"pos", "\[LeftDoubleBracket]", 
             RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}], " ", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"Exp", "[", 
                RowBox[{
                 RowBox[{"-", "bumpWidth"}], " ", 
                 SuperscriptBox[
                  RowBox[{"(", 
                   RowBox[{"#1", "-", "t"}], ")"}], "2"]}], "]"}], "&"}], 
              ")"}], "/@", 
             RowBox[{"pos", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", "1"}], "\[RightDoubleBracket]"}]}]}], 
           ")"}]}]}], ";", 
        RowBox[{
         RowBox[{"controlForce", "[", "t_", "]"}], "=", 
         RowBox[{
          RowBox[{"gains", "[", "t", "]"}], ".", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"x", "[", "t", "]"}], ",", 
            RowBox[{
             SuperscriptBox["x", "\[Prime]",
              MultilineFunction->None], "[", "t", "]"}], ",", 
            RowBox[{
             RowBox[{"\[Theta]", "[", "t", "]"}], "-", "\[Theta]s"}], ",", 
            RowBox[{
             SuperscriptBox["\[Theta]", "\[Prime]",
              MultilineFunction->None], "[", "t", "]"}]}], "}"}]}]}], ";", 
        RowBox[{
         RowBox[{"sols", "[", "t_", "]"}], "=", 
         RowBox[{"First", "[", 
          RowBox[{"NDSolve", "[", 
           RowBox[{
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{
                 RowBox[{"L", " ", "m", " ", 
                  RowBox[{"Sin", "[", 
                   RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
                  SuperscriptBox[
                   RowBox[{
                    SuperscriptBox["\[Theta]", "\[Prime]",
                    MultilineFunction->None], "[", "t", "]"}], "2"]}], 
                 "\[Equal]", 
                 RowBox[{
                  RowBox[{"2", " ", 
                   RowBox[{"(", 
                    RowBox[{"c", "+", "m"}], ")"}], " ", 
                   RowBox[{
                    SuperscriptBox["x", "\[Prime]\[Prime]",
                    MultilineFunction->None], "[", "t", "]"}]}], "+", 
                  RowBox[{"L", " ", "m", " ", 
                   RowBox[{"Cos", "[", 
                    RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
                   RowBox[{
                    SuperscriptBox["\[Theta]", "\[Prime]\[Prime]",
                    MultilineFunction->None], "[", "t", "]"}]}], "+", 
                  RowBox[{"perturbingForce", "[", "t", "]"}], "+", 
                  RowBox[{"controlForce", "[", "t", "]"}]}]}], ",", 
                RowBox[{
                 RowBox[{"L", " ", "m", " ", 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"2", " ", 
                    RowBox[{"Sin", "[", 
                    RowBox[{"\[Theta]", "[", "t", "]"}], "]"}]}], "+", 
                    RowBox[{"2", " ", 
                    RowBox[{"Cos", "[", 
                    RowBox[{"\[Theta]", "[", "t", "]"}], "]"}], " ", 
                    RowBox[{
                    SuperscriptBox["x", "\[Prime]\[Prime]",
                    MultilineFunction->None], "[", "t", "]"}]}], "+", 
                    RowBox[{"L", " ", 
                    RowBox[{
                    SuperscriptBox["\[Theta]", "\[Prime]\[Prime]",
                    MultilineFunction->None], "[", "t", "]"}]}]}], ")"}]}], 
                 "\[Equal]", "0"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{
                 RowBox[{"x", "[", "0", "]"}], "\[Equal]", "x0"}], ",", 
                RowBox[{
                 RowBox[{
                  SuperscriptBox["x", "\[Prime]",
                   MultilineFunction->None], "[", "0", "]"}], "\[Equal]", 
                 "x00"}], ",", 
                RowBox[{
